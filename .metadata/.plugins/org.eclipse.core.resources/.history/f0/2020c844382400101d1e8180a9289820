public class Ejercicio1 {
	
	public static void main(String[] args) {
		System.out.println("kevo");
		System.out.println(calcularSueldo(15,20));
	}
	
	//Ejercicio 1.a
	public static int calcularSueldo(int horasTrabajadas, int precioHora) {
		return horasTrabajadas*precioHora;
	}
	
}


